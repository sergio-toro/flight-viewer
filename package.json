{
	"name": "flight-viewer",
	"productName": "Paragliding Flight Viewer",
	"version": "1.1.4",
	"main": "main/index.js",
	"scripts": {
		"clean": "rimraf dist renderer/.next renderer/out",
		"start": "cross-env NODE_OPTIONS='--inspect' electron .",
		"build": "next build renderer && next export renderer",
		"pack-app": "npm run clean && npm run build && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --dir",
		"dist": "npm run clean && npm run build && electron-builder"
	},
	"build": {
		"asar": true,
		"files": [
			"main",
			"renderer/out"
		]
	},
	"devDependencies": {
		"@date-io/date-fns": "^2.10.6",
		"@material-ui/core": "^4.11.2",
		"@material-ui/icons": "^4.11.2",
		"cross-env": "7.0.2",
		"electron-builder": "22.9.1",
		"eslint": "7.14.0",
		"eslint-config-airbnb": "18.2.1",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-react": "7.21.5",
		"eslint-plugin-react-hooks": "4.2.0",
		"igc-parser": "^0.5.0",
		"leaflet": "^1.7.1",
		"lerp": "1.0.3",
		"next": "10.0.3",
		"next-transpile-modules": "4.1.0",
		"prettier": "^2.2.1",
		"react": "17.0.1",
		"react-dom": "17.0.1",
		"react-promise-suspense": "0.3.3",
		"react-use-gesture": "8.0.1",
		"electron": "11.0.3",
		"styled-components": "^5.2.1"
	},
	"dependencies": {
		"electron-is-dev": "1.2.0",
		"electron-next": "3.1.5"
	},
	"license": "MIT"
}
